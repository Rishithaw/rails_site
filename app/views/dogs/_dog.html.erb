<div id="<%= dom_id dog %>">
  <p>
    <strong>Name:</strong>
    <%= dog.name %>
  </p>

  <p>
    <strong>Owner:</strong>
    <%= dog.owner_id %>
  </p>

  <p>
  <strong>Breed:</strong>
  <%= dog.sub_breed.breed.name %>
  </p>

  <p>
    <strong>Sub-breed:</strong>
    <%= dog.sub_breed.name %>
  </p>


</div>
